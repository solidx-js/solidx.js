{
  "name": "solidx.js",
  "version": "2.0.1",
  "description": "A New Way of 3D Programming",
  "files": [
    "dist"
  ],
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "dev": "vuepress dev docs -p 10001 --clean-temp --clean-cache",
    "build": "rm -rf dist/* && parcel build ./src/index.ts --no-cache --reporter @parcel/reporter-bundle-analyzer",
    "test": "jest -i",
    "ci": "prettier --check src && tsc -p tsconfig.json --noEmit && npm test",
    "release": "npm run ci && rm -rf dist/* && npm run build && npm run docs:build && release-it",
    "docs:build": "vuepress build docs",
    "tsdoc": "rm -rf docs/docs && typedoc"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "@parcel/config-default": "^2.10.3",
    "@parcel/packager-ts": "2.10.3",
    "@parcel/reporter-bundle-analyzer": "^2.10.3",
    "@parcel/transformer-less": "2.10.3",
    "@parcel/transformer-raw": "^2.10.3",
    "@parcel/transformer-svg-react": "^2.10.3",
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@parcel/transformer-typescript-types": "2.10.3",
    "@types/chroma-js": "^2.4.3",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@vuepress/bundler-vite": "2.0.0-rc.0",
    "@vuepress/bundler-webpack": "2.0.0-rc.0",
    "@vuepress/client": "2.0.0-rc.0",
    "@vuepress/plugin-back-to-top": "2.0.0-rc.0",
    "@vuepress/plugin-container": "2.0.0-rc.0",
    "@vuepress/plugin-search": "2.0.0-rc.0",
    "@vuepress/theme-default": "2.0.0-rc.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "parcel": "^2.10.3",
    "path-browserify": "^1.0.1",
    "prettier": "^3.1.0",
    "process": "^0.11.10",
    "release-it": "^17.0.0",
    "serve-static": "^1.15.0",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.2",
    "vue": "^3.3.9",
    "vuepress": "2.0.0-rc.0",
    "vuepress-plugin-auto-catalog": "2.0.0-rc.4"
  },
  "dependencies": {
    "@babylonjs/core": "^6.34.1",
    "@babylonjs/loaders": "^6.34.1",
    "@babylonjs/materials": "^6.34.1",
    "@lit/context": "^1.1.0",
    "ah-logger": "^3.2.8",
    "chroma-js": "^2.4.2",
    "lit": "^3.1.0",
    "lodash": "^4.17.21",
    "tslib": "^2.6.2"
  },
  "targets": {
    "global": {
      "outputFormat": "global"
    }
  }
}
