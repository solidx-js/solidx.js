    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "parcel": "^2.10.3",
    "prettier": "^3.1.0",
