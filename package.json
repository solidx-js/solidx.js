{
  "name": "bjs-frame",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "dev": "parcel serve --dist-dir=node_modules/.dist demo/*.html demo/assets/**/*",
    "build": "rm -rf dist/* && parcel build ./src/index.ts --no-cache",
    "test": "jest -i",
    "ci": "prettier --check src && tsc -p tsconfig.json --noEmit && npm test",
    "release": "npm run ci && rm -rf dist/* && npm run build && release-it",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babylonjs/inspector": "^6.31.0",
    "@parcel/config-default": "^2.10.3",
    "@parcel/packager-ts": "2.10.3",
    "@parcel/transformer-less": "2.10.3",
    "@parcel/transformer-raw": "^2.10.3",
    "@parcel/transformer-svg-react": "^2.10.3",
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@parcel/transformer-typescript-types": "2.10.3",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@vuepress/client": "2.0.0-rc.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "parcel": "^2.10.3",
    "path-browserify": "^1.0.0",
    "prettier": "^3.1.0",
    "process": "^0.11.10",
    "release-it": "^17.0.0",
    "typescript": "^5.2.2",
    "vue": "^3.3.9",
    "vuepress": "2.0.0-rc.0"
  },
  "dependencies": {
    "@babylonjs/core": "^6.31.0",
    "@lit/context": "^1.1.0",
    "ah-logger": "^3.2.8",
    "lit": "^3.1.0",
    "lodash": "^4.17.21"
  },
  "targets": {
    "global": {
      "outputFormat": "global"
    }
  }
}
